<nav class="navigation navigation--transparent" id="navigation">
  <div class="nav__logo">
    <a href="index.ejs"
      ><img src="../img/Logo.svg" alt="Logo" class="logo-svg"
    /></a>
  </div>
  <button
    aria-label="Menüknopf"
    class="menu-button mobile-menu-button"
    id="mobile-menu-button"
  >
    <img src="../img/Burger Menu.svg" alt="Menü" class="burger-menu-svg" />
  </button>
  <button
    aria-label="Menü schließen"
    class="menu-button close-menu-button u-hidden"
    id="mobile-menu-close-button"
  >
    <img
      src="../img/Burger Menu close.svg"
      alt="Menü schließen"
      class="burger-menu-svg--close"
    />
  </button>
  <div class="nav__container">
    <ul class="nav__list">
      <% [["das Café","cafe"],["Herkunft","origins"],["Shop","shop"]].forEach(function(item){ %>
        <li>
          <li class="menu-item <%if (locals.activePage == item[1] ) { %>active<% } %>">
            <a href=<%= item[1] + ".ejs" %> class="menu-link"
              ><%= item[0] %></a
            >  
        </li>
      <% }); %>
    </ul>
  </div>
</nav>
